{
	"name": "fruster-auth-service",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "ts-node-dev --respawn --transpileOnly ./app.ts",
		"start": "ts-node -T ./app.ts",
		"test": "nyc --reporter=html --check-coverage --lines 90 ts-node ./spec/support/jasmine-runner.js"
	},
	"dependencies": {
		"crypto": "^1.0.1",
		"csprng": "^0.1.2",
		"fruster-bus": "^0.5.3",
		"fruster-health": "0.0.13",
		"fruster-log": "0.0.18",
		"fruster-errors": "0.1.2",
		"jwt-simple": "^0.5.5",
		"lodash": "^4.17.11",
		"mongodb": "^2.2.34",
		"ms": "^2.1.1",
		"uuid": "^3.3.2",
		"cookie": "^0.3.1"
	},
	"devDependencies": {
		"@types/mongodb": "2.2.19",
		"fruster-test-utils": "^0.4.0",
		"jasmine": "^3.3.1",
		"jasmine-spec-reporter": "^4.2.1",
		"nyc": "^13.1.0",
		"ts-node": "^8.4.1",
		"ts-node-dev": "^1.0.0-pre.43",
		"typescript": "^3.6.3"
	},
	"engines": {
		"node": "8.1.2"
	},
	"nyc": {
		"exclude": [
			"spec/*",
			"lib/errors.js",
			"lib/deprecatedErrors.js",
			"lib/constants.js",
			"lib/docs.js",
			"conf.js"
		]
